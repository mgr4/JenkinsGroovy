node {
    stage('SCM') {
	echo 'Gathering code from version control'
        git branch: '${branch}', url: 'https://github.com/suvsap/JenkinsGroovy.git'
    }
    stage('Build') {
       try{
        echo 'Building....'
        sh 'wget https://packages.microsoft.com/config/ubuntu/20.04/packages-microsoft-prod.deb -O packages-microsoft-prod.de'
        sh 'sudo dpkg -i packages-microsoft-prod.deb'
        sh 'sudo apt-get -qq update && apt-get -qq --yes --force-yes install apt-transport-https'
        sh 'sudo apt-get -qq update && apt-get -qq --yes --force-yes install dotnet-sdk-2.1'
        sh 'dotnet --version'
        sh "dotnet build ConsoleApp1"
        echo 'Building New Feature'
        releasenotes()
    }catch(ex){
        echo 'Something went wrong...'
        echo ex.toString()
        currentBuild.result = 'Failure'
    }
     finally{
        //cleanup
     }
   }
    stage('Test') {
        echo 'Testing....'
    }
    stage('Deploy') {
        echo 'Deploying....'
    }
}
